<idea-plugin>
    <id>com.evgenysobko.diploma</id>
    <name>PluginsPerformance</name>
    <vendor>evgenysobko</vendor>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow id="myPlugin" anchor="bottom" secondary="true" doNotActivateOnStart="true"
                    factoryClass="com.evgenysobko.diploma.toolwindow.ToolWindowFactory"/>
    </extensions>

</idea-plugin>
